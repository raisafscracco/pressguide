<%- include ("../../partials/header.ejs"); %> 
<%- include("../../partials/navbar.ejs"); %>

<div class="container">
    <hr>
    <h2>Users</h2>
    <a class="btn btn-success" href="/admin/users/create">Create a New User</a>
    <hr>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>ID</th>
                <th>E-mail</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                  <td><%= user.id %></td>
                  <td><%= user.email %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</div>

<%- include ("../../partials/footer.ejs"); %>
